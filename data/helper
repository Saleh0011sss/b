	c = types.InlineKeyboardButton("Ø¯Ø±Ø¨Ø§Ø±Ù‡ Ù…Ø§", callback_data='about')  
	markup.add(c)
	helper.send_message(msg.chat.id, "Ø¨Ù‡ Ø±Ø¨Ø§Øª Ù‡Ù„Ù¾Ø± Ø®ÙˆØ´ Ø¢Ù…Ø¯ÛŒØ¯", reply_markup=markup)

